keywords = {Advanced glycation end products,antibody,bioactivity,glycation,methods,modification},
number = {4},
pages = {586--594},
pmid = {28272973},
publisher = {Taylor {\&} Francis},
title = {{Glycation of antibodies: Modification, methods and potential effects on biological functions}},
