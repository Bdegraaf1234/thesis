issn = {15700232},
keywords = {Alternatives to protein A,Antibody,Aqueous two-phase separation,Centrifugation,Chromatography,Depth filtration,Disposable technology,Future,Industrial,Large scale,Manufacturing facilities,Membrane chromatography,Membrane filtration,Monoclonal,PAT,Protein A,Protein crystallization,Protein precipitation,Purification,Viral clearance},
month = {mar},
