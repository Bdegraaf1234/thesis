issn = {1460-2059},
journal = {Bioinformatics},
month = {sep},
number = {17},
pages = {2174--2180},
pmid = {19535534},
title = {{Automated protein (re)sequencing with MS/MS and a homologous database yields almost full coverage and accuracy}},
