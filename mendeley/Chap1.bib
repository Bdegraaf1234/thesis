journal = {Cell Host and Microbe},
keywords = {elderly population,immunoglobulin genes,influenza vaccine,monoclonal antibodies},
month = {mar},
number = {3},
pages = {357----366.e6},
pmid = {30795982},
publisher = {Cell Press},
title = {{Influenza Virus Vaccination Elicits Poorly Adapted B Cell Responses in Elderly Individuals}},
