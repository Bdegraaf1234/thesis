pages = {1--14},
pmid = {26589402},
publisher = {BioMed Central Ltd.},
title = {{Practical guidelines for B-cell receptor repertoire sequencing analysis}},
url = {https://link.springer.com/articles/10.1186/s13073-015-0243-2 https://link.springer.com/article/10.1186/s13073-015-0243-2},
volume = {7},
year = {2015}
}
