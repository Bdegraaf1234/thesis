issn = {2073-4468},
journal = {Antibodies (Basel, Switzerland)},
keywords = {antibody engineering,therapeutic biologics},
month = {dec},
number = {4},
pages = {55},
pmid = {31816964},
title = {{Antibody Structure and Function: The Basis for Engineering Therapeutics.}},
url = {/pmc/articles/PMC6963682/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6963682/ https://www.mdpi.com/2073-4468/8/4/55 http://www.ncbi.nlm.nih.gov/pubmed/31816964 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6963682},
