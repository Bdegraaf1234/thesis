month = {may},
number = {10},
pages = {4285--4289},
pmid = {1350088},
title = {{Humanization of an anti-p185HER2 antibody for human cancer therapy.}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.89.10.4285 http://www.ncbi.nlm.nih.gov/pubmed/1350088 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC49066},
volume = {89},
