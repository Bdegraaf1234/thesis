pages = {1},
pmid = {31894001},
publisher = {BioMed Central Ltd.},
title = {{Development of therapeutic antibodies for the treatment of diseases}},
url = {https://jbiomedsci.biomedcentral.com/articles/10.1186/s12929-019-0592-z http://www.ncbi.nlm.nih.gov/pubmed/31894001 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6939334},
