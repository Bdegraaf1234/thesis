doi = {10.1038/emboj.2009.401},
issn = {0261-4189},
journal = {The EMBO journal},
number = {4},
pages = {717--726},
publisher = {Nature Publishing Group},
title = {{Architecture of the RNA polymerase II-TFIIF complex revealed by cross-linking and mass spectrometry.}},
