journal = {Journal of Chromatography A},
keywords = {Cross-linking,Diagonal chromatography,Mass spectrometry,Nuclear extract,Structural proteomics},
pages = {34--46},
publisher = {Elsevier B.V.},
title = {{Isolation of cross-linked peptides by diagonal strong cation exchange chromatography for protein complex topology studies by peptide fragment fingerprinting from large sequence databases}},
