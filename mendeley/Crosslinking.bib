journal = {Journal of proteome research},
keywords = {70 kda bacterial heat,apidaecin,mass spectrometry,pramp,proline-rich antimicrobial peptide,protein targets,ribosome,shock protein dnak},
number = {8},
pages = {3274--3283},
title = {{Identification of Api88 Binding Partners in Escherichia coli Using a Photoaffinity-Cross-Link Strategy and Label-Free Quantification.}},
