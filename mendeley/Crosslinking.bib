journal = {Molecular {\&} cellular proteomics : MCP},
number = {X},
title = {{Characterization of Dynamic UbR-Proteasome Subcomplexes by In vivo Cross-linking (X) Assisted Bimolecular Tandem Affinity Purification (XBAP) and Label-free Quantitation.}},
