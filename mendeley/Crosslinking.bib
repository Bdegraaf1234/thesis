title = {{Characterization and Identification of Dityrosine Cross-Linked Peptides Using Tandem Mass Spectrometry}},
url = {file:///Users/helgehoffmann/Library/Application Support/Mendeley Desktop/Downloaded/Mukherjee et al. - 2017 Characterization and Identification of Dityrosine Cross-Linked Peptides Using Tandem Mass Spectrometry.pdf},
volume = {89},
