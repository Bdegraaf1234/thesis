keywords = {Automated data analysis,Chemical cross-linking,Cleavable cross-linkers,Mass spectrometry,Protein 3D-structure,Software},
number = {1},
pages = {83--97},
title = {{Automated assignment of MS/MS cleavable cross-links in protein 3d-structure analysis}},
url = {file:///Users/helgehoffmann/Library/Application Support/Mendeley Desktop/Downloaded/G{\"{o}}tze et al. - 2014 Automated assignment of MSMS cleavable cross-links in protein 3d-structure analysis.pdf},
