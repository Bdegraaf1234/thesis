isbn = {1535-3907 (Electronic){\$}\backslash{\$}r1535-3893 (Linking)},
issn = {15353907},
journal = {Journal of Proteome Research},
pages = {3241−3248},
pmid = {24874765},
title = {{De Novo protein sequencing by combining top-down and bottom-up tandem mass spectra}},
