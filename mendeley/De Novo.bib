keywords = {algorithms,combinatorial optimization,combinatorics,dynamic programming,machine learning,markov chains,mass spectroscopy,statistics},
number = {8},
pages = {140--153},
title = {{Spectrum fusion: Using multiple mass spectra for de novo peptide sequencing}},
volume = {4955 LNBI},
