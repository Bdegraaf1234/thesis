pages = {1151--1161},
pmid = {28348172},
publisher = {American Society for Biochemistry and Molecular Biology},
title = {{Database-independent Protein Sequencing (DiPS) Enables Full-length de Novo Protein and Antibody Sequence Determination}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28348172 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5461544 https://linkinghub.elsevier.com/retrieve/pii/S1535947620323768},
