@article{Muhlbacher2014,
abstract = {During transcription initiation at promoters of protein-coding genes, RNA polymerase (Pol) II assembles with TBP, TFIIB and TFIIF into a conserved core initiation complex that recruits additional factors. The core complex stabilizes open DNA and initiates RNA synthesis, and it is conserved in the Pol I and Pol III transcription systems. Here, we derive the domain architecture of the yeast core pol II initiation complex during transcription initiation. The yeast complex resembles the human initiation complex and reveals that the TFIIF Tfg2 winged helix domain swings over promoter DNA. An 'arm' and a 'charged helix' in TFIIF function in transcription start site selection and initial RNA synthesis, respectively, and apparently extend into the active centre cleft. Our model provides the basis for further structure-function analysis of the entire transcription initiation complex.},
