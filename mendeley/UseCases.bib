doi = {10.7554/eLife.02978},
issn = {2050-084X (Electronic){\$}\backslash{\$}r2050-084X (Linking)},
journal = {eLife},
keywords = {centromeres,kinetochores,mitosis},
pages = {e02978----e02978},
title = {{The pseudo GTPase CENP-M drives human kinetochore assembly}},
url = {file:///Users/helgehoffmann/Library/Application Support/Mendeley Desktop/Downloaded/Basilico et al. - 2014 The pseudo GTPase CENP-M drives human kinetochore assembly.pdf},
